{
  "name": "keysense",
  "version": "0.1.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "ios": "expo run:ios",
    "android": "expo run:android",
    "web": "expo start --web",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "validate:exercises": "ts-node scripts/validate-exercises.ts",
    "generate:exercise": "ts-node scripts/generate-exercise.ts",
    "measure:latency": "ts-node scripts/measure-latency.ts",
    "build:ios": "eas build --platform ios",
    "build:android": "eas build --platform android",
    "build:preview": "eas build --profile preview"
  },
  "dependencies": {
    "@react-navigation/bottom-tabs": "^7.0.0",
    "@react-navigation/native": "^7.0.0",
    "expo": "~52.0.0",
    "expo-haptics": "~14.0.0",
    "expo-router": "~4.0.0",
    "expo-splash-screen": "^31.0.13",
    "expo-status-bar": "~2.0.0",
    "react": "18.3.1",
    "react-native": "0.76.5",
    "react-native-gesture-handler": "~2.20.0",
    "react-native-mmkv": "^3.0.2",
    "react-native-reanimated": "~3.16.0",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.0.0",
    "zustand": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.0",
    "@testing-library/react-native": "^12.7.0",
    "@types/jest": "^29.5.0",
    "@types/react": "~18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "eslint-config-expo": "~8.0.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "jest-expo": "~52.0.0",
    "prettier": "^3.3.0",
    "ts-node": "^10.9.0",
    "typescript": "~5.6.0"
  }
